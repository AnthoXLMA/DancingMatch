<div class="navbar-yourjob" style="box-shadow: 10px 3px 25px #bababa;  justify-content:space-between;">
  <div class="logo">
    <div class="logo-text">
      <h1>Dancing Partners</h1>
    </div>
<%= link_to "Home", root_path %>
  </div>
</div>
<div class="title">
    <h1>Our Dancing Community</h1>
</div>

  <div class="partners">
    <% @partners.each do |partner| %>
    <div class="card-partners">
      <h2><%= link_to partner.pseudo, partner_path(partner) %></h2>
    </div>
    <% end %>
  </div>
  <%= link_to "Home", root_path %>
<div id="map"
   style="width: 100%; height: 600px;"
   data-markers="<%= @markers.to_json %>"
   data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
</div>
