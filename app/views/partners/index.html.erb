<div class="contain">
  <div class="title">
    <h1>Meet all dancers</h1>
  </div>

  <div class="partners">
    <% @partners.each do |partner| %>
    <div class="card-partner">
      <h3><%= link_to partner.name, partner_path(partner) %></h3>
      <h2><%= partner.dance %></h2>
    </div>
    <% end %>
  </div>
</div>
