<div class="navbar-yourjob" style="box-shadow: 10px 3px 25px #bababa;  justify-content:space-between;">
  <div class="logo">
    <div class="logo-text">
      <h1>Dancing Partners</h1>
    </div>
  </div>
<%= link_to "Back", :back %>
<%= link_to "Home", root_path %>
<%= link_to "My profile", profile_path %>
</div>
<div class="container-partner">
  <div class="top-card">
    <h1><%= @partner.pseudo %></h1>
    <h2><%= link_to @move.title , dance_path(@dances) %></h2>
  </div>
  <div class="infos-partner">
    <h3><%= @partner.gender %></h3>
    <h3><%= @partner.age %> years old</h3>
    <h3><%= @partner.experience %> years experience</h3>
    <h3><%= @partner.location %></h3>
  </div>
</div>
<div class="contact-partner">
  <h4>Contact this dancer</h4>
</div>

